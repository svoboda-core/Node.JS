<!DOCTYPE html>
<html lang="ru">

<%- include('./partials/haed.ejs') %>

<body>
	<!-- Подключаем меню используя шаблонизатор -->
	<%- include('./partials/nav.ejs') %>
		<h1>Contacts:</h1>
		<ul>
			<% if (contacts.length) { %>
				<% contacts.forEach(({ link, name })=> { %>
					<li>
						<a href="<%= link %>"><%= name %>
						</a>
					</li>
					<% }) %>
				<% } %>
		</ul>
</body>

</html>